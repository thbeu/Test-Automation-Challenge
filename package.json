{
  "name": "test_automation_challenge",
  "version": "1.0.0",
  "description": "Automation framework for automation challenge",
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "@reportportal/agent-js-cypress": "^5.1.0",
    "chai-sorted": "0.2.0",
    "cypress": "10.9.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-wait-until": "1.7.2",
    "fs-extra": "^10.1.0",
    "immutable": "^3.8.2",
    "mocha": "9.2.2",
    "multiple-cucumber-html-reporter": "^1.19.0",
    "path-browserify": "^1.0.1",
    "prettier": "2.6.0"
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "yarn": "^1.22.19"
  },
  "scripts": {
    "node:generate_cucumber_report": "node custom-reporter.js",
    "test:run_test": "npx cypress open --env TAGS=\"\""
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
